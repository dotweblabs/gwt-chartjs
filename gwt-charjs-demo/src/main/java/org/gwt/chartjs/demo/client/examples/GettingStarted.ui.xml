<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:l="urn:import:org.gwt.chartjs.demo.client.examples.line">
	<ui:style>
		body{
			padding:0;
			margin:0;
			font-family: 'Noto Sans', sans-serif !important;
		}
		.root {
			position: static !important;
			overflow: visible;
			border-top: 8px #282b36 solid;
			color: #282b36;
		}
		
		.root * {
			color: #282b36;
			adding: 0;
			margin: 0;
			color: inherit;
			-webkit-font-smoothing: antialiased;
			text-rendering: optimizeLegibility;
		}
		
		.redColor,.greenColor,.yellowColor {
			width: 33.33%;
			float: left;
			height: 8px;
		}
		
		.redColor {
			background-color: #f33e6f;
		}
		
		.greenColor {
			background-color: #46bfbd;
		}
		
		.yellowColor {
			background-color: #fdb45c;
		}
	</ui:style>
	<g:HTMLPanel stylePrimaryName="{style.root}">
		<div>
			<div class="{style.redColor}" />
			<div class="{style.greenColor}" />
			<div class="{style.yellowColor}" />
		</div>
		<h1>Getting started</h1>
		<l:LineChartExample></l:LineChartExample>
	</g:HTMLPanel>
</ui:UiBinder> 