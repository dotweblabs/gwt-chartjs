<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:org.gwt.chartjs.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<c:LineChart ui:field="lineChart" pixelWidth="500" pixelHeight="300" />
		<g:Button text="update" ui:field="updateButton" />
		<g:Button text="reload" ui:field="reloadButton" />
		<g:Button text="view sources" ui:field="viewSourceButton" />
	</g:HTMLPanel>
</ui:UiBinder> 